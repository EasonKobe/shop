		<result column="cstNm" property="cstNm" />
		<result column="coreCstId" property="coreCstId" /><!-- 核心系统或ecif客户编号 -->
		<result column="cmisCstId" property="cmisCstId" /><!-- 信贷系统客户编号 -->
