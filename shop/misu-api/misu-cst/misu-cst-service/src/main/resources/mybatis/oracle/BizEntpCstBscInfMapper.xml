		BCS.DEL_IND AS DEL_IND,BCS.VERSION AS VERSION,BCS.RMNDR_MOD_CD AS RMNDR_MOD_CD,BCS.RMNDR_IND AS RMNDR_IND,BCS.TENANT_ID AS TENANT_ID,
